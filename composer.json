{
	"name": "leo-unglaub/contao-group-description",
	"description": "A very small extension for the Contao CMS that allows you to add a description/notes to your member and user groups.",
	"keywords": [
		"contao",
		"group-description",
		"user",
		"member",
		"description"
	],
	"type": "contao-module",
	"license": "LGPL-3.0+",
	"authors": [
		{
			"name": "Leo Unglaub",
			"email": "leo@unglaub.at",
			"homepage": "https://leo.unglaub.at",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://bitbucket.org/leo-unglaub/contao-group-description/issues",
		"source": "https://bitbucket.org/leo-unglaub/contao-group-description/src/",
		"email": "leo@leo-unglaub.net",
		"irc": "irc://irc.freenode.org/contao.dev"
	},
	"require": {
		"php": "^5.6 || ^7.0",
		"contao/core": "~3",
		"contao-community-alliance/composer-installer": "*"
	},
	"replace": {
		"contao-legacy/group-description": "*"
	},
	"extra": {
		"contao": {
			"sources": {
				"src/system/modules/group-description": "system/modules/group-description"
			}
		}
	}
}
